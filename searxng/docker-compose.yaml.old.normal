version: '3'
services:
  searxng:
    image: searxng/searxng
    container_name: searxng
    restart: always
    ports:
      - 8181:8080
    volumes:
      - ${PWD}/searxng:/etc/searxng
    environment:
      - INSTANCE_NAME=Pardesi-Search
      - BASE_URL=https://search.pardesicat.xyz/
      - AUTOCOMPLETE=google
      - AUTOCOMPLETE_MIN=2
